);\n  },\n  methods: {\n    getArticles() {\n      return axios\n        .get(\"http://localhost:3000/article\")\n        .then((res) => {\n          this.articles = res.data.articles;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    pickRandomNumber() {\n      return (this.randomNumber = Math.floor(\n        Math.random() * this.articles.length\n      ));\n    },\n    getRandomArticle() {\n      this.pickRandomNumber();\n      this.article = { ...this.articles[this.randomNumber] };\n      this.articles.splice(this.randomNumber, 1);\n      console.log(this.articles.length);\n    },\n  },\n};\n</script>\n\n<style scoped>\n#content {\n  margin: 30vh 20vw 20vh 20vw;\n}\np {\n  font-family: Helvetica, sans-serif;\n  font-size: 1.4rem;\n}\nbutton {\n  border: none;\n  background-color: #fff;\n  font-size: 2rem;\n  cursor: pointer;\n}\n.title {\n  font-family: Arial, Helvetica, sans-serif;\n  font-size: 2.8rem;\n}\n.preview {\n  font-family: \"Times New Roman\", Times, serif;\n  font-size: 1.8rem;\n  text-align: end;\n  margin: 5vh 0;\n}\n.paragraphs p {\n  margin: 2vh 10% 0 10%;\n  text-align: justify;\n}\n.toutLu {\n  margin-top: 50vh;\n  transform: translateY(-50%);\n  text-align: center;\n}\n</style>\n"],"sourceRoot":""}]}